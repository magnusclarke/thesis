@article{thomas_pastis:_2013,
	title = {{PASTIS:} an R package to facilitate phylogenetic assembly with soft taxonomic inferences},
	volume = {4},
	copyright = {© 2013 The Authors. Methods in Ecology and Evolution © 2013 British Ecological Society},
	issn = {2041-{210X}},
	shorttitle = {{PASTIS}},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/2041-210X.12117/abstract},
	doi = {10.1111/2041-210X.12117},
	abstract = {* Phylogenetic trees that include all member lineages are necessary for many questions in macroevolution, biogeography and conservation. Currently, producing such trees when genetic data or phenotypic characters for some tips are missing generally involves assigning missing species to the root of their most exclusive clade, essentially grafting them onto existing and static topologies as polytomies. * We describe an R package, {‘PASTIS’}, that enables a two-stage Bayesian method using {MrBayes} version 3.2 (or higher) to incorporate lineages lacking genetic data at the tree inference stage. The inputs include a consensus topology, a set of taxonomic statements (e.g. placing species in genera and aligning some genera with each other or placing subspecies within species) and user-defined priors on edge lengths and topologies. {PASTIS} produces input files for execution in {MrBayes} that will produce a posterior distribution of complete ultrametric trees that captures uncertainty under a homogeneous birth-death prior model of diversification and placement constraints. If the age distribution of a focal node is known (e.g. from fossils), the ultrametric tree distribution can be converted to a set of dated trees. We also provide functions to visualize the placement of missing taxa in the posterior distribution. * The {PASTIS} approach is not limited to the level of species and could equally be applied to higher or lower levels of organization (e.g. accounting for all recognized subspecies or populations within a species) given an appropriate choice of priors on branching times.},
	language = {en},
	number = {11},
	urldate = {2013-11-24},
	journal = {Methods in Ecology and Evolution},
	author = {Thomas, Gavin H. and Hartmann, Klaas and Jetz, Walter and Joy, Jeffrey B. and Mimoto, Aki and Mooers, Arne O.},
	year = {2013},
	keywords = {bioinformatics, phylogenetics, reconstruction, software},
	pages = {1011–1017},
	file = {Full Text PDF:/home/magnus/.zotero/zotero/vruwxia5.default/zotero/storage/WZURRMJT/Thomas et al. - 2013 - PASTIS an R package to facilitate phylogenetic as.pdf:application/pdf;Snapshot:/home/magnus/.zotero/zotero/vruwxia5.default/zotero/storage/MHCDQI3I/full.html:text/html}
},

@article{hernandez_using_2013,
	title = {Using phylogenetic information and the comparative method to evaluate hypotheses in macroecology},
	volume = {4},
	copyright = {© 2013 The Authors. Methods in Ecology and Evolution © 2013 British Ecological Society},
	issn = {2041-{210X}},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/2041-210X.12033/abstract},
	doi = {10.1111/2041-210X.12033},
	abstract = {* It is widely recognized that macroecological patterns are not independent of the evolution of the lineages involved in generating these patterns. While many researchers have begun to evaluate the effect of ancestor–descendant relationships on observed patterns using the phylogenetic comparative method, most macroecological studies only utilize the cross-sectional comparative method to ‘remove the phylogenetic history’, without considering the option of evaluating its effect without removing it. * Currently, most researchers use this method without explicitly evaluating three fundamental evolutionary assumptions of the comparative method: (i) that the phylogeny is constructed without error (which implies evaluating phylogenetic uncertainty); (ii) that more closely related species tend to show more similar characters than expected by chance (which implies evaluating the phylogenetic signal) and; (iii) that the model of the characters' evolution effectively recapitulates their history (which implies comparing the fit of several evolutionary models and evaluating the uncertainty of the estimating model parameters). * Macroecological studies will benefit from the use of the comparative method to assess the effect of phylogenetic history without removing its effect. The comparative method will also allow for the simultaneous analysis of trait evolution and its impact on diversification rates; it is important to evaluate these processes together because they are not independent. In addition, explicit evaluations of the assumptions of comparative methods using Bayesian inferences will allow researchers to quantify the uncertainty of specific evolutionary hypotheses accounting for observed macroecological patterns. * We illustrate the usefulness of the method using the phylogeny of the genus Sebastes (Pisces: Scorpaeniformes), together with data on the body size–latitudinal range relationship to estimate the effect of phylogenetic history on the observed macroecological pattern.},
	language = {en},
	number = {5},
	urldate = {2013-10-08},
	journal = {Methods in Ecology and Evolution},
	author = {Hernández, Cristián E. and Rodríguez-Serrano, Enrique and Avaria-Llautureo, Jorge and Inostroza-Michael, Oscar and Morales-Pallero, Bryan and Boric-Bargetto, Dusan and Canales-Aguirre, Cristian B. and Marquet, Pablo A. and Meade, Andrew},
	year = {2013},
	keywords = {Bayesian inference, character evolution, directional comparative method, diversification rate, macroevolution, {PGLS}, phylogenetics},
	pages = {401–415},
	file = {Full Text PDF:/home/magnus/.zotero/zotero/vruwxia5.default/zotero/storage/BN2J7D8N/Hernández et al. - 2013 - Using phylogenetic information and the comparative.pdf:application/pdf;Snapshot:/home/magnus/.zotero/zotero/vruwxia5.default/zotero/storage/G3S57TRV/abstract.html:text/html;Snapshot:/home/magnus/.zotero/zotero/vruwxia5.default/zotero/storage/M4AF7B3A/abstract.html:text/html}
},

