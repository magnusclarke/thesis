},

@article{blackburn_adaptive_2013,
	title = {An Adaptive Radiation of Frogs in a Southeast Asian Island Archipelago},
	volume = {67},
	copyright = {© 2013 The Authors. Evolution published by Wiley Periodicals, Inc. on behalf of The Society for the Study of Evolution., This is an open access article under the terms of the Creative Commons Attribution License, which permits use, distribution and reproduction in any medium, provided the original work is properly cited.},
	issn = {1558-5646},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/evo.12145/abstract},
	doi = {10.1111/evo.12145},
	abstract = {Living amphibians exhibit a diversity of ecologies, life histories, and species-rich lineages that offers opportunities for studies of adaptive radiation. We characterize a diverse clade of frogs (Kaloula, Microhylidae) in the Philippine island archipelago as an example of an adaptive radiation into three primary habitat specialists or ecotypes. We use a novel phylogenetic estimate for this clade to evaluate the tempo of lineage accumulation and morphological diversification. Because species-level phylogenetic estimates for Philippine Kaloula are lacking, we employ dense population sampling to determine the appropriate evolutionary lineages for diversification analyses. We explicitly take phylogenetic uncertainty into account when calculating diversification and disparification statistics and fitting models of diversification. Following dispersal to the Philippines from Southeast Asia, Kaloula radiated rapidly into several well-supported clades. Morphological variation within Kaloula is partly explained by ecotype and accumulated at high levels during this radiation, including within ecotypes. We pinpoint an axis of morphospace related directly to climbing and digging behaviors and find patterns of phenotypic evolution suggestive of ecological opportunity with partitioning into distinct habitat specialists. We conclude by discussing the components of phenotypic diversity that are likely important in amphibian adaptive radiations.},
	language = {en},
	number = {9},
	urldate = {2013-10-10},
	journal = {Evolution},
	author = {Blackburn, David C. and Siler, Cameron D. and Diesmos, Arvin C. and {McGuire}, Jimmy A. and Cannatella, David C. and Brown, Rafe M.},
	year = {2013},
	keywords = {Comparative methods, disparity, diversification, ecomorphology, Kaloula, microhylidae},
	pages = {2631–2646},
	file = {Full Text PDF:/home/magnus/.zotero/zotero/vruwxia5.default/zotero/storage/VQINKKFQ/Blackburn et al. - 2013 - An Adaptive Radiation of Frogs in a Southeast Asia.pdf:application/pdf;Snapshot:/home/magnus/.zotero/zotero/vruwxia5.default/zotero/storage/WHDJE9TA/full.html:text/html}
},

