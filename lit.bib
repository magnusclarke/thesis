	journal = {Methods in Ecology and Evolution},
	author = {Guénard, Guillaume and Legendre, Pierre and Peres-Neto, Pedro},
	year = {2013},
	keywords = {comparative method, cross-validation, evolutionary models, graph theory, Ornstein-Uhlenbeck process, phylogenetic eigenvectors, phylogenetic modelling, phylogenetic signal, statistical modelling, trait values},
	pages = {n/a–n/a},
	file = {Full Text PDF:/home/magnus/.zotero/zotero/vruwxia5.default/zotero/storage/KK53HGZ3/Guénard et al. - 2013 - Phylogenetic eigenvector maps (PEM) a framework t.pdf:application/pdf;Snapshot:/home/magnus/.zotero/zotero/vruwxia5.default/zotero/storage/6TWZDXHT/abstract.html:text/html}
},

