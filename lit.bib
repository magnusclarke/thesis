	file = {Full Text PDF:/home/magnus/.zotero/zotero/vruwxia5.default/zotero/storage/DWTD3F83/Thomas and Freckleton - 2012 - MOTMOT models of trait macroevolution on trees.pdf:application/pdf;Snapshot:/home/magnus/.zotero/zotero/vruwxia5.default/zotero/storage/IP89VXG8/abstract.html:text/html}
},

@article{nakagawa_missing_2008,
